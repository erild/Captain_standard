{
  "name": "Project",
  "plural": "Projects",
  "base": "PersistedModel",
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": false,
      "validateUpsert": true,
      "silenceWarnings": false
    }
  },
  "properties": {
    "id": {
      "type": "number",
      "id": true,
      "generated": false
    },
    "name": {
      "type": "string",
      "required": true
    },
    "cloneUrl": {
      "type": "string",
      "required": true
    },
    "configCmd": {
      "type": "string",
      "default": "",
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "linters": {
      "type": "hasMany",
      "model": "Linter",
      "foreignKey": "projectId",
      "through": "ProjectLinter"
    },
    "customers": {
      "type": "hasAndBelongsToMany",
      "model": "Customer",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "create"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "linters-exec"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "delProjectLinters"
    }
  ],
  "methods": {
    "delProjectLinters": {
      "isStatic": false,
      "accepts": [],
      "returns": [
        {
          "arg": "count",
          "type": "number",
          "root": false,
          "description": "Number of row deleted"
        }
      ],
      "description": "Delete the project's Linter relations",
      "http": [
        {
          "path": "/linters/delAllRel",
          "verb": "delete"
        }
      ]
    }
  }
}
