{
  "name": "Project",
  "plural": "Projects",
  "base": "PersistedModel",
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": false,
      "validateUpsert": true,
      "silenceWarnings": false
    },
    "CascadeDelete": {
      "relations": ["linters", "customers"]
    }
  },
  "properties": {
    "id": {
      "type": "number",
      "id": true,
      "generated": false
    },
    "fullName": {
      "type": "string",
      "required": true,
      "default": ""
    },
    "cloneUrl": {
      "type": "string",
      "required": true
    },
    "configCmd": {
      "type": "string",
      "required": false,
      "default": ""
    },
    "webhookSecret": {
      "type": "string",
      "required": false,
      "default": ""
    }
  },
  "validations": [],
  "relations": {
    "linters": {
      "type": "hasMany",
      "model": "Linter",
      "foreignKey": "projectId",
      "through": "ProjectLinter"
    },
    "customers": {
      "type": "hasAndBelongsToMany",
      "model": "Customer",
      "foreignKey": ""
    },
    "scripts": {
      "type": "hasMany",
      "model": "Script",
      "foreignKey": "projectId",
      "through": "ProjectScript"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "findById"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "upsert"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "__link__customers"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "updateAllRel"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "integration-hook"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "deleteById"
    }
  ],
  "methods": {
  }
}
