{
  "name": "Project",
  "plural": "Projects",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required" : false,
      "validateUpsert": true,
      "silenceWarnings": false
    }
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "remoteId": {
      "type": "number",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "linters": {
      "type": "hasAndBelongsToMany",
      "model": "Linter",
      "foreignKey": "projectId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "create"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
