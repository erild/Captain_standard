{
  "name": "Linter",
  "plural": "Linters",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": false,
      "validateUpsert": true,
      "silenceWarnings": false
    }
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "languages": {
      "type": [
        "string"
      ],
      "required": true,
      "default": [
        "['javascript']"
      ]
    },
    "installCmd": {
      "type": "string",
      "required": true
    },
    "runCmd": {
      "type": "string",
      "required": true
    },
    "pathToParser": {
      "type": "string",
      "required": true,
      "default": "../../server/linters-results-parsers/default-parser"
    }
  },
  "validations": [],
  "relations": {
    "creator": {
      "type": "hasOne",
      "model": "Customer"
    },
    "projects": {
      "type": "hasMany",
      "model": "Project",
      "foreignKey": "linterId",
      "through": "ProjectLinter"
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "__get__creator"
    }
  ],
  "methods": {}
}
